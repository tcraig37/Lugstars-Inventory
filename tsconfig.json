    "strict": false,
    "jsx": "react-jsx",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "lib": ["ES2020", "DOM"],
    "target": "ES2020",
    "module": "ESNext",
    "moduleResolution": "node",
    "noImplicitAny": false
